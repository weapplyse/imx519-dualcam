					i2c@0 {
						status = "okay";
						reg = <0>;
						#address-cells = <1>;
						#size-cells = <0>;

						serializer_0: serializer@42 {
							status = "okay";
							compatible = "videtronic,v-link-ser";
							reg = <0x42>;
